; Editor Configuration Normalization
;      http://editorconfig.org/

root                        = true

[*]
charset                     = UTF-8
end_of_line                 = LF
indent_size                 = 4
indent_style                = tab
insert_final_newline        = true
tab_width                   = 4
trim_trailing_whitespace    = true

[*.md]
trim_trailing_whitespace    = false
